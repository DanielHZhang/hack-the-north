{
  "name": "react-node-template",
  "version": "0.1.0",
  "description": "React node template for vanilla JS",
  "author": "Daniel Zhang",
  "main": "server/server.js",
  "private": true,
  "scripts": {
    "start": "nodemon server/index.js",
    "start:i": "cross-env nodemon --config nodemon.register.json",
    "start:api": "cross-env API_ONLY=true nodemon --config nodemon.register.json",
    "serve": "cross-env NODE_ENV=production node dist/server.bundle.js",
    "build:dll": "webpack --config webpack.config.common.js --progress --colors",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --colors",
    "clean:build": "rm -rf dist/bundle && rm -rf .awcache",
    "clean:node": "yarn cache clean && rm -rf node_modules && yarn",
    "cbs": "yarn clean && yarn build && yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stats": "webpack --config webpack.config.prod.js --stats > stats.json"
  },
  "dependencies": {
    "antd": "^3.9.2",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "btoa": "^1.2.1",
    "connect-mongo": "^2.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fb": "^2.0.0",
    "firebase": "^5.5.0",
    "google-map-react": "^1.0.6",
    "mongoose": "^5.2.9",
    "node-sass": "^4.9.3",
    "nodemailer": "^4.6.8",
    "particlesjs": "^2.2.3",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-spring": "^5.7.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.3",
    "@babel/core": "^7.0.0-rc.3",
    "@babel/node": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/polyfill": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.1.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.3",
    "react-hot-loader": "^4.3.5",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "simple-progress-webpack-plugin": "^1.1.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3"
  }
}
